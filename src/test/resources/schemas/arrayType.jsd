{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "arrays",
  "type": "object",
  "properties": {
    "arrayString": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "maxItems": 3
    },
    "arrayNumber": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "maxItems": 3,
      "minItems": 1
    },
    "arrayInteger": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "maxItems": 3,
      "minItems": 1
    },
    "arrayBoolean": {
      "type": "array",
      "items": {
        "type": "boolean"
      },
      "maxItems": 3,
      "minItems": 1
    },
    "arrayObject": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "prop1": {
            "type": "integer"
          },
          "prop2": {
            "type": "string"
          }
        }
      },
      "maxItems": 3,
      "minItems": 1
    }
  },
  "required": [
    "arrayString", "arrayBoolean", "arrayInteger", "arrayNumber", "arrayObject"
  ]
}